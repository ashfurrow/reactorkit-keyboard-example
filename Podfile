# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'reactorkitKeyboardExample' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  inhibit_all_warnings!
  # Pods for reactorkitKeyboardExample
  
  # Architecture
  pod 'ReactorKit'

  # UI
  pod 'Then'
  pod 'SnapKit'
  pod 'ReusableKit'
  pod 'AcknowList'  
  
  # Rx
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxViewController'
  pod 'RxDataSources'
  pod 'RxOptional'

  # Logging
  pod 'CocoaLumberjack/Swift'

  target 'keyboard' do
    inherit! :search_paths
  end 

end
